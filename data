pipeline {
   agent any
   stages {
      stage('BUILD') {
         steps {
         echo "this is build stage"
       }
    }
	  parallel {
          stage('DEPLOY') {
         steps {
         echo "this is deploy stage"
       }
     } 
  }
      stage('TEST') {
         steps {
         echo "this is test stage"
       }
    }
 }
}
